{
  "name": "@modular-agent/common-utils",
  "version": "1.0.0",
  "description": "Common utilities for Modular Agent Framework",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./http/errors": {
      "types": "./dist/http/errors.d.ts",
      "import": "./dist/http/errors.js"
    },
    "./http/transport": {
      "types": "./dist/http/transport.d.ts",
      "import": "./dist/http/transport.js"
    },
    "./http": {
      "types": "./dist/http/index.d.ts",
      "import": "./dist/http/index.js"
    },
    "./id-utils": {
      "types": "./dist/utils/id-utils.d.ts",
      "import": "./dist/utils/id-utils.js"
    },
    "./timestamp-utils": {
      "types": "./dist/utils/timestamp-utils.d.ts",
      "import": "./dist/utils/timestamp-utils.js"
    },
    "./result-utils": {
      "types": "./dist/utils/result-utils.d.ts",
      "import": "./dist/utils/result-utils.js"
    },
    "./token-encoder": {
      "types": "./dist/utils/token-encoder.d.ts",
      "import": "./dist/utils/token-encoder.js"
    },
    "./evalutor/path-resolver": {
      "types": "./dist/evalutor/path-resolver.d.ts",
      "import": "./dist/evalutor/path-resolver.js"
    },
    "./evalutor/condition-evaluator": {
      "types": "./dist/evalutor/condition-evaluator.d.ts",
      "import": "./dist/evalutor/condition-evaluator.js"
    },
    "./llm/wrapper": {
      "types": "./dist/llm-clients/base-client.d.ts",
      "import": "./dist/llm-clients/base-client.js"
    },
    "./observable": {
      "types": "./dist/llm-clients/message-stream.d.ts",
      "import": "./dist/llm-clients/message-stream.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@modular-agent/types": "workspace:*",
    "zod": "4.3.6"
  },
  "devDependencies": {}
}